{
  "_instructions": "Use this payload with the paperform_create_form MCP tool in Claude.ai, or POST to https://api.paperform.co/v1/forms with Bearer auth.",
  "title": "Let's Get Started — A.I.M.S.",
  "slug": "aims-onboarding-needs-analysis",
  "description": "Tell us about yourself and what you need. ACHEEVY will have your environment ready in minutes.",
  "active": true,
  "theme": {
    "background_color": "#F8FAFC",
    "accent_color": "#F59E0B",
    "font_family": "Inter",
    "button_color": "#F59E0B",
    "button_text_color": "#1E293B"
  },
  "settings": {
    "show_progress_bar": true,
    "allow_partial_submissions": true,
    "redirect_url": "https://plugmein.cloud/chat",
    "success_message": "Welcome to A.I.M.S.! ACHEEVY is reviewing your submission now. Head to chat to get started."
  },
  "pages": [
    {
      "title": "About You",
      "fields": [
        { "type": "text", "key": "full_name", "title": "Full Name", "required": true, "description": "Your full name" },
        { "type": "email", "key": "email", "title": "Email", "required": true },
        { "type": "text", "key": "company", "title": "Company / Organization", "required": false },
        {
          "type": "select", "key": "role", "title": "Your Role", "required": true,
          "options": ["Founder / CEO", "CTO / Technical Lead", "Marketing / Growth", "Operations", "Product Manager", "Developer", "Freelancer / Consultant", "Other"]
        },
        {
          "type": "select", "key": "industry", "title": "Industry", "required": false,
          "options": ["Technology", "E-commerce", "Healthcare", "Finance", "Education", "Real Estate", "Media / Entertainment", "Professional Services", "Non-profit", "Other"]
        },
        {
          "type": "select", "key": "team_size", "title": "Team Size", "required": false,
          "options": ["Just me", "2-5", "6-20", "21-50", "51-200", "200+"]
        }
      ]
    },
    {
      "title": "What Brings You Here?",
      "fields": [
        {
          "type": "multiselect", "key": "needs", "title": "What do you need help with?", "required": true,
          "options": ["Deploy an AI tool or agent", "Build a custom application", "Automate repetitive tasks", "Manage infrastructure / containers", "Research & analysis", "Content creation & publishing", "Client onboarding & intake", "Financial tracking & billing", "Other"]
        },
        {
          "type": "textarea", "key": "primary_goal", "title": "Primary Goal", "required": true,
          "placeholder": "In one sentence, what would success look like for you?"
        },
        {
          "type": "textarea", "key": "pain_point", "title": "What's your biggest pain point right now?", "required": false,
          "placeholder": "What's taking too much time, costing too much, or just not working?"
        }
      ]
    },
    {
      "title": "Tool Deployment",
      "condition": "needs contains 'Deploy an AI tool or agent'",
      "fields": [
        {
          "type": "multiselect", "key": "tool_type", "title": "What kind of tool are you looking for?", "required": true,
          "options": ["AI chatbot / assistant", "Research agent", "Code generation tool", "Data analysis platform", "Content creation tool", "Monitoring / observability", "Communication bot (Telegram, Discord, Slack)", "Custom agent", "Not sure — help me decide"]
        },
        {
          "type": "select", "key": "hosting_pref", "title": "Hosting preference", "required": true,
          "options": ["Managed by A.I.M.S. (recommended)", "Self-hosted (I'll run it myself)", "Hybrid (managed + export option)"]
        },
        {
          "type": "select", "key": "usage_scale", "title": "Expected usage scale", "required": false,
          "options": ["Light (testing / personal)", "Medium (small team, daily use)", "Heavy (production, high traffic)", "Enterprise (multi-tenant, SLA required)"]
        }
      ]
    },
    {
      "title": "Custom Build",
      "condition": "needs contains 'Build a custom application'",
      "fields": [
        {
          "type": "textarea", "key": "build_description", "title": "What are you building?", "required": true,
          "placeholder": "Describe the app, tool, or platform you want to build."
        },
        {
          "type": "text", "key": "current_stack", "title": "Current tech stack", "required": false,
          "placeholder": "e.g., React, Node.js, PostgreSQL, AWS"
        },
        {
          "type": "select", "key": "has_code", "title": "Do you have existing code?", "required": true,
          "options": ["Yes — I have a repo", "Partially — some prototype/wireframes", "No — starting from scratch"]
        },
        {
          "type": "multiselect", "key": "integrations", "title": "Integration requirements", "required": false,
          "options": ["Stripe / payments", "Email (Gmail, SendGrid)", "Slack / Discord", "CRM (Salesforce, HubSpot)", "Database (Postgres, MongoDB, Firebase)", "AI / LLM APIs", "Calendar", "Cloud storage", "None / Not sure"]
        }
      ]
    },
    {
      "title": "Automation",
      "condition": "needs contains 'Automate repetitive tasks'",
      "fields": [
        {
          "type": "textarea", "key": "automation_tasks", "title": "What tasks do you want to automate?", "required": true,
          "placeholder": "Describe the manual process you want automated."
        },
        {
          "type": "select", "key": "task_frequency", "title": "How often does this task run?", "required": true,
          "options": ["Multiple times per day", "Daily", "Weekly", "Monthly", "On-demand / event-triggered"]
        },
        {
          "type": "text", "key": "current_tools", "title": "Current tools involved", "required": false,
          "placeholder": "e.g., Google Sheets, email, Slack, manual copy-paste"
        },
        {
          "type": "select", "key": "automation_urgency", "title": "Urgency", "required": false,
          "options": ["Critical — costing money/time daily", "Important — would save significant effort", "Nice-to-have — efficiency improvement", "Exploring — just curious what's possible"]
        }
      ]
    },
    {
      "title": "Timeline & Budget",
      "fields": [
        {
          "type": "select", "key": "timeline", "title": "Timeline", "required": true,
          "options": ["ASAP — need it this week", "1-2 weeks", "1 month", "2-3 months", "No rush — exploring"]
        },
        {
          "type": "select", "key": "budget", "title": "Monthly budget range", "required": false,
          "options": ["Free tier / testing ($0)", "Starter ($25-100/mo)", "Growth ($100-500/mo)", "Pro ($500-2,000/mo)", "Enterprise ($2,000+/mo)", "Not sure — help me estimate"]
        },
        {
          "type": "select", "key": "wants_consultation", "title": "Would you like a free consultation?", "required": false,
          "options": ["Yes — book a 15-min call", "Maybe later", "No thanks — just get started"]
        }
      ]
    },
    {
      "title": "Book a Consultation",
      "condition": "wants_consultation equals 'Yes — book a 15-min call'",
      "fields": [
        {
          "type": "datetime", "key": "consultation_datetime", "title": "Preferred date/time", "required": true,
          "description": "Select a time in the next 14 days during business hours"
        },
        {
          "type": "select", "key": "contact_method", "title": "Preferred contact method", "required": true,
          "options": ["Video call (Zoom/Meet)", "Phone call", "Chat on A.I.M.S."]
        },
        {
          "type": "textarea", "key": "consultation_notes", "title": "Anything specific you want to discuss?", "required": false
        }
      ]
    }
  ],
  "calculated_fields": [
    {
      "key": "calc_tier",
      "logic": "Based on budget + usage_scale + team_size → Free / Starter / Growth / Pro / Enterprise"
    },
    {
      "key": "calc_path",
      "logic": "Based on needs → Deploy It (tool deployment) / Guide Me (custom build) / Automate It (automation)"
    },
    {
      "key": "calc_priority",
      "logic": "Based on timeline + automation_urgency + budget → 1-10 scale"
    },
    {
      "key": "calc_luc_estimate",
      "logic": "Based on calc_tier + usage_scale → monthly LUC token estimate"
    }
  ]
}
