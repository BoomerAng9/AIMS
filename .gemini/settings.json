{
  "model": "gemini-3.1-pro",
  "thinking": {
    "enabled": true,
    "level": "MEDIUM"
  },
  "context": {
    "fileName": "GEMINI.md"
  },
  "mcpServers": {
    "stitch": {
      "description": "Google Stitch — AI UI design and code generation",
      "command": "npx",
      "args": ["-y", "@google/stitch-mcp"],
      "env": {
        "STITCH_API_KEY": "${STITCH_API_KEY}",
        "STITCH_PROJECT_ID": "aims-frontend"
      }
    },
    "nano-banana": {
      "description": "Nano Banana Pro — Gemini 3 Pro Image generation via MCP",
      "command": "npx",
      "args": ["-y", "nano-banana-mcp"],
      "env": {
        "GEMINI_API_KEY": "${GEMINI_API_KEY}"
      }
    },
    "n8n": {
      "description": "n8n workflow automation — read, create, update workflows",
      "command": "node",
      "args": ["./n8n-mcp/dist/index.js"],
      "env": {
        "N8N_HOST": "${N8N_HOST}",
        "N8N_API_KEY": "${N8N_API_KEY}"
      }
    }
  },
  "extensions": [
    {
      "name": "stitch",
      "url": "https://github.com/gemini-cli-extensions/stitch",
      "autoUpdate": true
    }
  ],
  "codeExecution": {
    "enabled": true,
    "sandbox": true
  },
  "safety": {
    "requireApproval": [
      "file_write",
      "shell_execute",
      "stitch_generate"
    ]
  }
}
